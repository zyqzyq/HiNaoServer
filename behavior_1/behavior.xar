                                    <![CDATA[import socket
class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        #put initialization code here
        self.a = True
        self.v = 50

    def onUnload(self):
        #put clean-up code here
        self.a = False
        self.s.close()

    def onInput_onStart(self):
        #self.onStopped() #activate the output of the box
        b = 10024
        address = ('0.0.0.0', b)
        self.s = socket.socket(socket.AF_INET, socket.SOCK_DGRAM)
        try:
            self.s.bind(address)
        except:
            b = b+1
        self.log(b)
        while self.a:
            data, addr = self.s.recvfrom(2048)
            if not data:
                print "client has exist"
                break
            print "received:", data, "from", addr
            if data[:2]=='00':
                #停止信号
                self.onStopped()
            elif data[0:2]=='01':
                #start run behavior
                self.output1(data[2:])
            elif data[0:2]=='02':
                #say or answer
                self.output2(data[2:])
            elif data[0:2]=='03':
                #move
                self.output3(data[2:])
            elif data[0:2]=='04':
                #get behaviorlist
                self.output4(data[2:])
        self.s.close()
    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]>
