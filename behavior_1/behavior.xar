                                                        <![CDATA[import time

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)

    def onLoad(self):
        self.bIsRunning = False
        self.bMustStop = False

    def onUnload(self):
        self.bMustStop = True
        self.outputX(0.0)
        self.outputY(0.0)
        self.outputTheta(0.0)

    def onInput_onStart(self):
        if( self.bIsRunning ):
            return
        self.bIsRunning = True
        self.bMustStop = False
        while( not self.bMustStop ):
            self.outputX(self.getParameter("X"))
            self.outputY(self.getParameter("Y"))
            self.outputTheta(self.getParameter("Theta"))
            self.outputStepFrequency(self.getParameter("Step frequency"))
            if( not self.bMustStop ):
                time.sleep(self.getParameter("Period of direction update (s)"))
        self.bIsRunning = False

    def onInput_onStop(self):
        self.onUnload()]]>
